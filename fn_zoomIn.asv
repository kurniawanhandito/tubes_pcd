function imgZoom = fn_zoomIn(img, times)
%fn_zoomIn Summary of this function goes here
%   Detailed explanation goes here
[row, col, page] = size(img);
new_row = row*times;
new_col = col*times;

for i=1:page
    for j=1:new_row
        for k=1:new_col
            imgZoom(j, k, i)=img(floor(j/times)+1 , floor(k/times), page);
        end
    end
end